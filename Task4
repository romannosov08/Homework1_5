a=set(input("Введите фамилии школьников, решивших задачу по алгебре:").split())
g=set(input("Введите фамилии школьников, решивших задачу по геометрии:").split())
t=set(input("Введите фамилии школьников, решивших задачу по тригонометрии:").split())
x=a&g&t
if len(x)==0:
  print("Все три задачи никто не решил")
else:
  for i in sorted (x):
    print(i, end=" ")
